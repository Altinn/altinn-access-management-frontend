{
  "name": "access-management",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "vite & wait-on http://localhost:5173 & cypress open",
    "coverage": "cypress run --component",
    "coverage:e2e": "vite & wait-on http://localhost:5173 & cypress run",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@altinn/altinn-design-system": "^0.27.6",
    "@digdir/design-system-react": "^0.11.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@tanstack/react-query": "^4.0.5",
    "axios": "^1.3.4",
    "classnames": "^2.3.2",
    "cypress": "^12.8.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "i18next": "^22.4.13",
    "lint-staged": "^13.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "redux-logger": "^3.0.6",
    "typescript-plugin-css-modules": "^5.0.0",
    "vite-plugin-svgr": "^2.4.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
