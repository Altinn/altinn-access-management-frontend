  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": ".NET on remote Altinn.AccessManagement.UI",
      "type": "coreclr",
      "request": "attach",
      "processName": "Altinn.AccessManagement.UI.exe" 
    },
    {
      "name": ".NET on remote seelct process ",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickRemoteProcess}",
    }
  ]
}